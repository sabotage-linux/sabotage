[build]
CC="$CC -static" ./configure --prefix=/ --disable-nls || exit 1
# __BEOS__ to use a shipped ar.h
make -j$MAKE_THREADS CFLAGS=-D__BEOS__ || exit 1
make DESTDIR="$butch_install_dir" install || exit 1
