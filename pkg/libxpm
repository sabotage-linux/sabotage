[build]
sed -i 's/USE_GETTEXT="yes"/USE_GETTEXT="no"/' configure || exit 1
CFLAGS="-D_GNU_SOURCE -D_BSD_SOURCE" ./configure -C --prefix="$butch_prefix" || exit 1
make DESTDIR="$butch_install_dir" -j$MAKE_THREADS install || exit 1

