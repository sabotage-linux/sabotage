[build]
patch -p1 < "$K"/kernel-headers_tcphdr.patch || exit 1
make ARCH="$A" prefix="$butch_prefix" DESTDIR="$butch_install_dir" install \
  || exit 1
dest="$butch_install_dir""$butch_prefix"
#fix ext2 header...
cp -f "$K"/ext2_fs.h "$dest"/include/linux/

